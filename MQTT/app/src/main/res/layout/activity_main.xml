<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rashminpc.mqtttest.MainActivity">

    <TextView
        android:id="@+id/lg"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="...."
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lt" />

    <TextView
        android:id="@+id/longt"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Longtitude"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/lg"
        app:layout_constraintTop_toBottomOf="@+id/latit" />

    <TextView
        android:id="@+id/tt"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="...."
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/temp"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Temperature"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/tt"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/latit"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Latitude"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/lt"
        app:layout_constraintTop_toBottomOf="@+id/humid3" />

    <TextView
        android:id="@+id/humid3"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Humidity"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/th"
        app:layout_constraintTop_toBottomOf="@+id/temp" />

    <TextView
        android:id="@+id/lt"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="...."
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/th" />

    <TextView
        android:id="@+id/th"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="...."
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tt" />

    <TextView
        android:id="@+id/name_speed"
        android:layout_width="180dp"
        android:layout_height="60dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Speed"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/value_speed"
        app:layout_constraintTop_toBottomOf="@+id/longt" />

    <TextView
        android:id="@+id/value_speed"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="...."
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lg" />

    <Button
        android:id="@+id/btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="Button"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_speed" />

</android.support.constraint.ConstraintLayout>
